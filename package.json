{
  "name": "node-ts",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsx ./src/bin/www.ts",
    "dev": "tsx watch ./src/bin/www.ts",
    "deploy": "pm2 startOrRestart pm2.config.js --env prod",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts  --fix --no-ignore",
    "lint:quiet": "eslint . --ext .ts  --quiet",
    "lint:strict": "eslint . --ext .ts  --fix --max-warnings=0",
    "migrate": "prisma migrate dev",
    "migrate:user": "npx prisma migrate dev --schema=./prisma/schema/user.prisma",
    "pull": "prisma db pull",
    "push": "prisma db push",
    "seed": "prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.2",
    "@types/ejs": "^3.1.5",
    "@types/eslint__js": "^9.14.0",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "child_process": "^1.0.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "_moduleAliases": {
    "@src": "src",
    "@helper": "src/_helper",
    "@controller": "src/_controller"
  }
}
